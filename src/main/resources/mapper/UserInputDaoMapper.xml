<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uclibm.ixn.dao.UserInputDao">
    <insert id="addContent" parameterType="String">
        insert into userinputs(content) values( #{content})
    </insert>
    <select id="getAllContents" resultType="String">
        select content from userinputs
    </select>
</mapper>